  },
  "accessToken": {
    "dataSource": "MongoDB",
    "public": false
  },
  "userCredential": {
    "dataSource": "MongoDB",
    "public": false
  },
  "userIdentity": {
    "dataSource": "MongoDB",
    "public": true
  },
  "user": {
    "dataSource": "MongoDB",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "ACL": {
    "dataSource": "MongoDB",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "MongoDB",
    "public": false
  },
  "Role": {
    "dataSource": "MongoDB",
    "public": false
  },
  "restaurant": {
    "dataSource": "MongoDB",
    "public": true
  },
  "menu": {
    "dataSource": "MongoDB",
    "public": true
  },
  "feedback": {
    "dataSource": "MongoDB",
    "public": true
  },
  "cart": {
    "dataSource": "MongoDB",
    "container": {
      "dataSource": "storage",
      "public": true
    }
  },
  "Email": {
    "dataSource": "email"
  },
  "order": {
    "dataSource": "MongoDB",
    "container": {
      "dataSource": "storage",
      "public": true
    }
  },
  "notifications": {
    "dataSource": "MongoDB",
    "public": true
  },

  "orderNotification": {
    "dataSource": "MongoDB",
    "public": true
  }
